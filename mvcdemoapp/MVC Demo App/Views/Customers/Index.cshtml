@model MVC_Demo_App.ViewModel.RandomMovieViewModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>List of Customers</h2>
<ul>
    @foreach (var customer in Model.Customers)
    {
        <li>
            @Html.ActionLink(customer.Name, "Detail", "Customers", new { Id = 1 }, null)
        </li>
    }
</ul>

